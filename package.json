{
  "name": "supabase-langchain-notion",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Mayooear",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "scrape-embed": "tsx -r dotenv/config scripts/scrape-embed.ts",
    "test-train-api": "tsx -r dotenv/config scripts/test-train-api.ts",
    "test:embed": "tsx -r dotenv/config scripts/test-embed-api.ts",
    "test:insights": "tsx -r dotenv/config scripts/test-insights-api.ts",
    "test:redis": "tsx -r dotenv/config scripts/test-redis-connection.ts",
    "check:queue": "tsx -r dotenv/config scripts/check-queue-status.ts",
    "worker": "tsx -r dotenv/config scripts/start-worker.ts",
    "check-schema": "tsx -r dotenv/config scripts/check-schema.ts",
    "pdf-to-markdown": "tsx scripts/pdf-to-markdown.ts",
    "train:markdown": "tsx -r dotenv/config scripts/train-markdown-files.ts",
    "test:embedding-search": "tsx -r dotenv/config scripts/test-embedding-search.ts"
  },
  "dependencies": {
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.3",
    "@langchain/openai": "^1.0.0",
    "@langchain/textsplitters": "^1.0.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@supabase/supabase-js": "^2.10.0",
    "bullmq": "^5.0.0",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "ioredis": "^5.3.2",
    "langchain": "^1.0.3",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "stripe": "^14.25.0",
    "tailwind-merge": "^1.10.0",
    "use-chat-submit": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.3",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "starter",
    "notion",
    "typescript",
    "nextjs",
    "langchain",
    "openai"
  ]
}
